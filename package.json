{
  "name": "ultra-mcp",
  "version": "0.1.3",
  "main": "dist/cli.js",
  "bin": {
    "ultra": "dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsup && chmod +x dist/cli.js",
    "dev": "tsup --watch",
    "build:tsc": "tsc --noEmit",
    "test": "vitest run --exclude '**/e2e/**'",
    "test:all": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --exclude '**/e2e/**'",
    "test:e2e": "vitest run src/__tests__/e2e",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "openai",
    "gemini",
    "gemini-cli",
    "claude-code"
  ],
  "author": "Mike Chong <oss@realmikechong.com>",
  "license": "MIT",
  "description": "Ultra MCP, a MCP server for using OpenAI and Gemini Models, and Gemini CLI with a single MCP. Use node.js, and Vercel AI SDK",
  "repository": {
    "type": "git",
    "url": "https://github.com/RealMikeChong/ultra-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/RealMikeChong/ultra-mcp/issues"
  },
  "homepage": "https://github.com/RealMikeChong/ultra-mcp#readme",
  "devDependencies": {
    "@types/node": "^22.15.34",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "vi-fetch": "^0.8.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@ai-sdk/azure": "^1.3.23",
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/openai": "^1.3.22",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "ai": "^4.3.16",
    "chalk": "^4.1.2",
    "colorette": "^2.0.20",
    "commander": "^14.0.0",
    "conf": "^14.0.0",
    "prompts": "^2.4.2",
    "zod": "^3.25.67"
  }
}
